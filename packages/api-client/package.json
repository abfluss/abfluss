    "description": "Api Client and helper for FlowApi",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "private": false,
    "keywords": [
        "flow",
        "polar",
        "api",
        "types"
    ],
    "homepage": "https://abfluss.github.io/abfluss/",
    "author": {
        "name": "donmahallem",
        "email": "donmahallem@users.noreply.github.com",
        "url": "https://github.com/donmahallem"
    },
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/abfluss/abfluss/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/abfluss/abfluss.git"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "@types/chai": "~4.2.11",
        "@types/mocha": "~7.0.2",
        "@types/node": "~13.11.1",
        "@types/request": "~2.48.0",
        "@types/request-promise-native": "~1.0.0",
        "@types/sinon": "~9.0.0",
        "chai": "~4.2.0",
        "mocha": "~7.1.1",
        "nyc": "~15.0.1",
        "sinon": "~9.0.2",
        "ts-node": "~8.8.2",
        "tslint": "~6.1.0",
        "typedoc": "~0.16.11",
        "typescript": "~3.8.3"
    },
    "dependencies": {
        "@abfluss/api-types": "^0.2.0",
        "@abfluss/util": "^0.2.0",
        "jsonschema": "~1.2.6",
        "request": "~2.88.0",
        "request-promise-native": "~1.0.7"
    },
    "files": [
        "dist/*",
        "src/*"
    ],
    "scripts": {
        "build": "tsc",
        "test": "mocha --config ../../.mocharc.yml",
        "test:watch": "mocha --config ../../.mocharc.yml --watch",
        "test:coverage": "nyc --nycrc-path ../../.nycrc.json npm run test",
        "lint": "tslint -c tslint.json -p tsconfig.lint.json src/**/*.ts",
        "lint:fix": "tslint --fix -c tslint.json -p tsconfig.lint.json src/**/*.ts",
        "docs": "typedoc --options ../../typedoc.json"
    }
